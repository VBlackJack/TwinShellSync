{
  "id": "1b80de48-f3d2-402f-85f6-951ba748d9b9",
  "name": "dmesg",
  "platform": "Linux",
  "commandPattern": "dmesg --level={level} --human --color=always | tail -n {lines}",
  "parameters": [
    {
      "name": "level",
      "label": "Niveau",
      "type": "string",
      "defaultValue": "err,warn",
      "required": true,
      "description": "Niveau de s\u00E9v\u00E9rit\u00E9 (emerg, alert, crit, err, warn, notice, info, debug)"
    },
    {
      "name": "lines",
      "label": "Nombre de lignes",
      "type": "number",
      "defaultValue": "100",
      "required": true,
      "description": "Nombre de lignes \u00E0 afficher"
    }
  ]
}